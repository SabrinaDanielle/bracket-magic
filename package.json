{
  "name": "bracket-magic",
  "version": "0.1.0",
  "description": "react boiler plate created by me, SDG",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack serve --open true  --config webpack.config.dev.js ",
    "dev:hot": "npm run dev -- --hot ",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "eslint ./ --fix"
  },
